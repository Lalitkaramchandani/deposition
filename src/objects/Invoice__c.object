        <fullName>Block_Void_Status_For_Due_Invoices</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW()==false &amp;&amp; ISCHANGED(Status__c) &amp;&amp; TEXT(Status__c)==&apos;Void&apos; &amp;&amp; Due_Amount__c&gt;0</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You can&apos;t void an invoice with a pending open balance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Date_Can_Not_be_In_Past</fullName>
        <active>false</active>
        <description>When selecting the invoice date, don’t allow invoices to be dated in prior months. For example, on February 15th, an invoice can be created for Feb 1, Feb 15 or Feb 28th but cannot be created for January 20th.</description>
        <errorConditionFormula>(ISNEW() || ISCHANGED(Invoice_Date__c))&amp;&amp;  ((MONTH(Invoice_Date__c)&lt;MONTH(TODAY()) &amp;&amp; YEAR(Invoice_Date__c) == YEAR(TODAY()) ) || YEAR(Invoice_Date__c) &lt;YEAR(TODAY()))</errorConditionFormula>
        <errorDisplayField>Invoice_Date__c</errorDisplayField>
        <errorMessage>Invoice can&apos;t be created for past month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Type_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type__c,&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Please enter a Type for this invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Invoice</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW()==false &amp;&amp; (ISCHANGED(Invoice_Date__c) ||
ISCHANGED(Deposition__c) ||
ISCHANGED(Attendee__c) ||
ISCHANGED(Type__c) ||
ISCHANGED(Status__c) ||
ISCHANGED(Deponent_Name__c) ||
ISCHANGED(Invoice_Amount__c) ||
ISCHANGED(Payment_Term_Days__c)


) &amp;&amp;  (MONTH( PRIORVALUE(Invoice_Date__c) ) &lt;  MONTH(TODAY())
||
YEAR(PRIORVALUE(Invoice_Date__c)) &lt; YEAR(TODAY()) )</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>You can&apos;t edit an invoice from a prior month.</errorMessage>
    </validationRules>
