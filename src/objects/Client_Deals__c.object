        <columns>NAME</columns>
        <columns>Actual_Close_Date__c</columns>
        <columns>Account_Name__c</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>Nature_of_Deal__c</columns>
        <columns>Services__c</columns>
        <columns>Geography__c</columns>
        <columns>VIP_Status__c</columns>
        <columns>Deal_Duration__c</columns>
        <columns>Won_Deal_Expiration__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Stage__c</field>
            <operation>contains</operation>
            <value>won</value>
        </filters>
        <filters>
            <field>Nature_of_Deal__c</field>
            <operation>equals</operation>
            <value>Exclusive Provider- Formal,Exclusive Provider - Formal / Renewal</value>
        </filters>
        <filters>
            <field>Services__c</field>
            <operation>includes</operation>
            <value>Reporting</value>
        </filters>
        <label>All Current (WON) Exclusive Reporting</label>
    </listViews>
    <listViews>
        <fullName>Lost_Client_Deals</fullName>
        <columns>NAME</columns>
        <columns>Account_Name__c</columns>
        <columns>Nature_of_Deal__c</columns>
        <columns>Services__c</columns>
        <columns>Geography__c</columns>
        <columns>OWNER.LAST_NAME</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Stage__c</field>
            <operation>contains</operation>
            <value>lost</value>
        </filters>
        <label>All Lost Deals</label>
    </listViews>
    <listViews>
        <fullName>Open_Deals</fullName>
        <columns>NAME</columns>
        <columns>Anticipated_Close_Date__c</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>Account_Name__c</columns>
        <columns>Nature_of_Deal__c</columns>
        <columns>Services__c</columns>
        <columns>Geography__c</columns>
        <columns>Stage__c</columns>
        <columns>Annualized_Revenue__c</columns>
        <columns>Probability__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Stage__c</field>
            <operation>notContain</operation>
            <value>LOST,WON</value>
        </filters>
        <label>All Open Deals</label>
    </listViews>
    <nameField>
        <label>Deal Name</label>
        <trackHistory>true</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Client Deals</pluralLabel>
    <searchLayouts>
        <searchResultsAdditionalFields>OWNER.FIRST_NAME</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>OWNER.LAST_NAME</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Close_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Stage__c ,&quot;Closed/Won&quot;)  &amp;&amp; 
ISBLANK( Actual_Close_Date__c )</errorConditionFormula>
        <errorDisplayField>Actual_Close_Date__c</errorDisplayField>
        <errorMessage>Please add the actual close date</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
