    <validationRules>
        <fullName>Approval_Restricted_to_Admins</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Status__c )
&amp;&amp; 
NOT(
$User.Username = &quot;aaron.brown@lexitas.sf&quot;  || 
$User.Username = &quot;kayla.lambert@lexitas.sf&quot;  || 
$User.Username = &quot;chad.davenport@lexitaslegal.com&quot;  || 
$User.Username = &quot;kvarallo@nextgenreporting.com&quot; 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Gifts can only be approved by Aaron, Chad, Ken or Kayla.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Description_Required</fullName>
        <active>true</active>
        <errorConditionFormula>isblank( Description_and_Reason__c)</errorConditionFormula>
        <errorDisplayField>Description_and_Reason__c</errorDisplayField>
        <errorMessage>Please explain the reason for this gift.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Giver_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Giver__c)</errorConditionFormula>
        <errorDisplayField>Giver__c</errorDisplayField>
        <errorMessage>Who is requesting this gift?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Gift_Preapproved</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT(ISPICKVAL(Status__c, &quot;Pending Approval&quot;))
&amp;&amp;
NOT(
$User.Username = &quot;aaron.brown@lexitas.sf&quot; ||
$User.Username = &quot;kayla.lambert@lexitas.sf&quot; ||
$User.Username = &quot;chad.davenport@lexitaslegal.com&quot; ||
$User.Username = &quot;kvarallo@nextgenreporting.com&quot;
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Gifts can only be approved by Aaron, Chad, Ken or Kayla.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ship_Date_Restricted_to_Admins</fullName>
        <active>true</active>
        <errorConditionFormula>(ISNEW() &amp;&amp; NOT(ISBLANK(Date_Shipped_if_applicable__c))
||
ISCHANGED( Date_Shipped_if_applicable__c ))
&amp;&amp; 
NOT(
$User.Username = &quot;aaron.brown@lexitas.sf&quot;  || 
$User.Username = &quot;kayla.lambert@lexitas.sf&quot;  || 
$User.Username = &quot;chad.davenport@lexitaslegal.com&quot;  || 
$User.Username = &quot;kvarallo@nextgenreporting.com&quot; 
)</errorConditionFormula>
        <errorDisplayField>Date_Shipped_if_applicable__c</errorDisplayField>
        <errorMessage>Only Aaron, Chad, Ken or Kayla can update the ship date</errorMessage>
    </validationRules>
