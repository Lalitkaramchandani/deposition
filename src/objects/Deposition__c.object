IF(INCLUDES(Deposition_Services__c, &quot;Process Serving&quot;),&quot;The client ordered a process server. &quot;, NULL) &amp; 
IF(INCLUDES(Deposition_Services__c, &quot;Expedite&quot;),&quot;This transcript was expedited. &quot;, NULL) &amp; 
IF(INCLUDES(Deposition_Services__c, &quot;Teleconference&quot;),&quot;Some of the parties were connected through a teleconference. &quot;, NULL) &amp; 
IF(text(Expedite__c)=&quot;None&quot;,&quot;&quot;,&quot;The expedite level on this &quot; &amp;  Event_Type_Lowercase__c &amp; &quot; is &quot; &amp;  Text(Expedite__c) &amp; &quot;. &quot;)</formula>
