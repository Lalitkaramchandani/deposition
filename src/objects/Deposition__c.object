    <validationRules>
        <fullName>Block_Blank_Special_Instruction_Text_Box</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Special_Instructions_text__c) &amp;&amp; 
Special_Instructions__c = TRUE</errorConditionFormula>
        <errorDisplayField>Special_Instructions__c</errorDisplayField>
        <errorMessage>Please fill in special instructions below.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_QC_Before_Production_Complete</fullName>
        <active>false</active>
        <errorConditionFormula>QC__c  = FALSE  &amp;&amp;   ISPICKVAL(Deposition_Status__c, &quot;Production Complete&quot;)</errorConditionFormula>
        <errorDisplayField>Deposition_Status__c</errorDisplayField>
        <errorMessage>Please QC before marking the job as complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Cannot_Be_Changed_on_Confirmed_Jobs</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Deposition_Date__c )  &amp;&amp; (

ISPICKVAL(Confirm_Client__c, &quot;Confirmed&quot;)  || 
ISPICKVAL(Confirm_Reporter__c, &quot;Confirmed&quot;)  ||  ISPICKVAL(Confirm_Interpreter__c, &quot;Confirmed&quot;)  ||  
ISPICKVAL( Confirm_Video_Tech__c , &quot;Confirmed&quot;) 
)</errorConditionFormula>
        <errorMessage>The deposition date cannot be changed on a confirmed job. To change the date, change the confirmation status for the client, reporter, videographer or interpreter.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Future_Dates</fullName>
        <active>false</active>
        <description>Make sure that deposition are scheduled with future dates.</description>
        <errorConditionFormula>NULLVALUE(DATEVALUE(CreatedDate),TODAY()) &gt;  Deposition_Date__c</errorConditionFormula>
        <errorMessage>The deposition date is in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hard_Copy_Needed</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Hard_Copy__c,&quot;Needed&quot;)
&amp;&amp;
ISPICKVAL(Deposition_Status__c,&quot;Production Complete&quot;)</errorConditionFormula>
        <errorMessage>It appears that one of the attendees needs a hard copy of the transcript. Once that&apos;s complete, please update the &quot;Hard Copy&quot; status to &quot;Complete&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Job_Confirmation_Requires_CR_Confirm</fullName>
        <active>false</active>
        <description>If the job involves a court reporter, they need to confirm before the job is confirmed.</description>
        <errorConditionFormula>INCLUDES(Deposition_Services__c, &quot;Court Reporter&quot;)  &amp;&amp; 
NOT(ISPICKVAL(Confirm_Reporter__c , &quot;Confirmed&quot;)) &amp;&amp;
ISPICKVAL( Deposition_Status__c , &quot;Confirmed&quot;)</errorConditionFormula>
        <errorDisplayField>Confirm_Reporter__c</errorDisplayField>
        <errorMessage>The job cannot be confirmed because the court reporter has not confirmed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Prod_Comp_Until_All_Is_Complete</fullName>
        <active>true</active>
        <description>The Deposition Status to cannot be changed to “Production Complete” if the Transcript, Exhibit or Video Status is “Not Received” or “In Production.”</description>
        <errorConditionFormula>ISPICKVAL(Deposition_Status__c,&quot;Production Complete&quot;) &amp;&amp;
(
ISPICKVAL(Video_Status__c,&quot;In Production&quot;) ||
ISPICKVAL(Video_Status__c,&quot;Not Received&quot;) ||
ISPICKVAL(Exhibit_Status__c,&quot;In Production&quot;) ||
ISPICKVAL(Exhibit_Status__c,&quot;Scanned&quot;) ||
ISPICKVAL(Exhibit_Status__c,&quot;Received&quot;) ||
ISPICKVAL(Exhibit_Status__c,&quot;Not Received&quot;) ||
ISPICKVAL(Transcript_Status__c,&quot;In Production&quot;) ||
ISPICKVAL(Transcript_Status__c,&quot;Not Received&quot;)
) 

&amp;&amp; 
$Profile.Name &lt;&gt; &quot;Public Site Profile&quot;</errorConditionFormula>
        <errorDisplayField>Deposition_Status__c</errorDisplayField>
        <errorMessage>The Deposition Status to cannot be changed to “Production Complete” if the Transcript, Exhibit or Video Status is “Not Received” or “In Production.”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Updates_on_Cancelled_Depos</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Deposition_Date__c)  &amp;&amp; 
 ISPICKVAL( Deposition_Status__c , &quot;Cancelled&quot;)</errorConditionFormula>
        <errorMessage>Dates cannot be changed on cancelled depositions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VidJob_Confirmation_Requires_Vid_Confirm</fullName>
        <active>false</active>
        <description>If the job involves a videographer, they need to confirm before the job is confirmed.</description>
        <errorConditionFormula>INCLUDES(Deposition_Services__c, &quot;Traditional Video&quot;)  &amp;&amp; 
NOT(ISPICKVAL( Confirm_Video_Tech__c  , &quot;Confirmed&quot;)) &amp;&amp;
ISPICKVAL( Deposition_Status__c , &quot;Confirmed&quot;)</errorConditionFormula>
        <errorDisplayField>Confirm_Video_Tech__c</errorDisplayField>
        <errorMessage>The job cannot be confirmed because the videographer has not confirmed.</errorMessage>
    </validationRules>
